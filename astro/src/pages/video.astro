---
import { RandomSnacksVid } from "../components/RandomSnacksVid";
import Layout from "../layouts/Layout.astro";
import { sanityAPI } from "../utils/sanity";
import type { SnacksVid } from "../types";

const result = await sanityAPI("videos");
const snacksVids = result.result as SnacksVid[];
---

<Layout title="Nasty Snacks | Video">
  <main>
    <RandomSnacksVid snacksVids={snacksVids} client:load />
  </main>
</Layout>
